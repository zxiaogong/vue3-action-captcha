    "@vue/test-utils": "^2.3.1",
    "happy-dom": "^8.9.0",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "rollup-plugin-delete": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.0.9"
